<%= simple_form_for(@nutrition) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :weight %>
    <%= f.input :height %>
    <%= f.input :identification %>
    <%= f.association :student %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
