<%= simple_form_for(@score) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :identification %>
    <%= f.input :period %>
    <%= f.input :area %>
    <%= f.input :score %>
    <%= f.association :student %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
