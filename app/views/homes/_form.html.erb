<%= simple_form_for(@home) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :paragraph %>
    <%= f.input :email %>
	<%= f.file_field :pictures %>
    <%= f.input :texto %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>