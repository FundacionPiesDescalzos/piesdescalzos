<%= simple_form_for(@student) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :id_type %>
    <%= f.input :gender %>
    <%= f.input :address %>
    <%= f.input :last_course %>
    <%= f.input :outschool_years %>
    <%= f.input :identification %>
    <%= f.input :born %>
    <%= f.input :etnic %>
    <%= f.input :villa %>
    <%= f.input :born_state %>
    <%= f.input :displaced %>
    <%= f.input :residency_state %>
    <%= f.input :zone %>
    <%= f.association :guardian %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
